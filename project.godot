[application]

config/name="7 of a kind"
config/description="A Touhou project fangame made in summer 2022."
run/main_scene="res://user-interface/menu/mainmenuPC.scn"
config/icon="res://icon.png"

[autoload]

Global="*res://autoload/Global.scn"
BulletManager="*res://godot-2d-bullet-hell/src/BulletManager.cs"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/vsync/use_vsync=false
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input]

focus={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777237,"physical_scancode":0,"unicode":0,"echo":false,"script":null)
 ]
}

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Enemy"

[physics]

common/enable_pause_aware_picking=true
2d/thread_model=2

[rendering]

threads/thread_model=2
misc/lossless_compression/webp_compression_level=9
batching/options/single_rect_fallback=true
environment/default_environment="res://default_env.tres"
