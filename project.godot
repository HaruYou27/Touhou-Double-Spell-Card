[application]

config/name="Touhou Entropy ~ Hakurei Chronos"
config/description="Why i make bullet shmups game."
run/main_scene="res://user-interface/mainMenu/Menu.scn"
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

Global="*res://autoload/Global.gd"
ItemManager="*res://autoload/ItemManager.cs"
